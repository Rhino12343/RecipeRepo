services:
  reciperepoapi:
    build:
      context: D:\nottingham_university_work\RecipeRepo
      dockerfile: RecipeRepo/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: RecipeRepo
      target: base
    container_name: RecipeRepo
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ConnectionStrings__db_connection_string: Server=reciperepodb,1433;Database=RecipeRepo;UID=sa;Trusted_Connection=False;MultipleActiveResultSets=True;Connect
        Timeout=10;
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      ASPNETCORE_db_connection_password: S00p3r53cr3t
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: ''
    image: reciperepo:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages  "/app/bin/Debug/net5.0/RecipeRepo.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - published: 8080
      target: 80
    - published: 80
      target: 80
    - published: 1443
      target: 443
    - published: 443
      target: 443
    tty: true
    volumes:
    - D:\nottingham_university_work\RecipeRepo\RecipeRepo:/app:rw
    - C:\Users\Adrian Bean\vsdbg\vs2017u5:/remote_debugger:rw
    - C:\Users\Adrian Bean\AppData\Roaming\ASP.NET\Https:/root/.aspnet/https:ro
    - C:\Users\Adrian Bean\AppData\Roaming\Microsoft\UserSecrets:/root/.microsoft/usersecrets:ro
    - C:\Users\Adrian Bean\.nuget\packages:/root/.nuget/packages:ro
    - D:\nottingham_university_work\RecipeRepo:/src:rw
  reciperepodb:
    environment:
      ACCEPT_EULA: "Y"
      MSSQL_PID: Developer
      SA_PASSWORD: S00p3r53cr3t
    image: mcr.microsoft.com/mssql/server
    ports:
    - published: 1433
      target: 1433
    volumes:
    - sqldata:/var/opt/mssql:rw
version: '3.4'
volumes:
  sqldata: {}